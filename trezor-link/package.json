{
  "name": "trezor-link",
  "version": "1.7.3",
  "description": "Trezor Link for browser",
  "main": "lib/index.js",
  "scripts": {
    "type-check": "flow check src",
    "lint": "eslint src",
    "build:lib": "./scripts/build-lib.sh",
    "publish:lib": "./scripts/publish-lib.sh",
    "test:unit": "jest",
    "test:unit:basic": "jest --testPathPattern basic.test --coverage false",
    "test:unit:old": "jest --testPathPattern old.test --coverage false",
    "test:unit:new": "jest --testPathPattern new.test --coverage false",
    "test:unit:new:debug": "node --inspect-brk node_modules/.bin/ts-jest --runInBand --testPathPattern new.test --coverage false",
    "test:integration:old": "jest --verbose -c jest.config.integration.js --testPathPattern bridge-old.integration",
    "test:integration:new": "jest --verbose -c jest.config.integration.js --testPathPattern bridge-new.integration",
    "build:new": "yarn tsc",
    "build:new:connect": "rm -rf ./lib && yarn build:new &&  rm -rf ../connect/trezor-link && mkdir ../connect/trezor-link && cp -rf ./lib ../connect/trezor-link/lib && cp ./yarn.lock package.json ../connect/trezor-link && cd ../connect/trezor-link && yarn"

  },
